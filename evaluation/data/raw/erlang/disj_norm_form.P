-file("test/lib/dialyzer/test/small_SUITE_data/src/disj_norm_form.erl",
      1).

-module(disj_norm_form).

-export([t/1]).

-record(foo,{bar}).

t(R) ->
    if
        R#foo.bar =:= 1;
        R#foo.bar =:= 2;
        R#foo.bar =:= 3;
        R#foo.bar =:= 4;
        R#foo.bar =:= 5;
        R#foo.bar =:= 6 ->
            ok;
        true ->
            error
    end.



