-file("test/lib/inets/test/ftp_ticket_test.erl", 1).

-module(ftp_ticket_test).

-compile(export_all).

init_per_testcase(Case, Config) ->
    ftp_suite_lib:init_per_testcase(Case, Config).

end_per_testcase(Case, Config) ->
    ftp_suite_lib:end_per_testcase(Case, Config).

all() ->
    tickets().

groups() ->
    [].

init_per_group(_GroupName, Config) ->
    Config.

end_per_group(_GroupName, Config) ->
    Config.

init_per_suite(Config) ->
    ftp_suite_lib:ftpd_init(ticket_test, Config).

tickets() ->
    [ticket_6035].

end_per_suite(Config) ->
    ftp_suite_lib:ftpd_fin(Config).

ticket_6035(X) ->
    ftp_suite_lib:ticket_6035(X).



