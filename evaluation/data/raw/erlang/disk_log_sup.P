-file("test/lib/kernel/src/disk_log_sup.erl", 1).

-module(disk_log_sup).

-behaviour(supervisor).

-export([start_link/0,init/1]).

start_link() ->
    supervisor:start_link({local,disk_log_sup}, disk_log_sup, []).

init([]) ->
    SupFlags = {simple_one_for_one,4,3600},
    Child =
        {disk_log,
         {disk_log,istart_link,[]},
         temporary,
         1000,
         worker,
         [disk_log]},
    {ok,{SupFlags,[Child]}}.



