-file("test/lib/dialyzer/test/small_SUITE_data/src/exhaust_case.erl", 1).

-module(exhaust_case).

-export([t/1]).

t(X) when is_integer(X) ->
    case ret(X) of
        foo ->
            ok;
        bar ->
            ok;
        42 ->
            ok;
        _other ->
            error
    end.

ret(1) ->
    foo;
ret(2) ->
    bar.



