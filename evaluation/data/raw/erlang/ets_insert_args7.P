-file("test/lib/dialyzer/test/race_SUITE_data/src/ets_insert_args7.erl",
      1).

-module(ets_insert_args7).

-export([test/0]).

test() ->
    Foo = foo,
    ets:new(Foo, [named_table,public]),
    race(Foo).

race(Tab) ->
    [{_,N}] = ets:lookup(Tab, counter),
    aux(Tab, N).

aux(Table, N) ->
    ets:insert(Table, [{counter,N + 1}]).



